{
  "name": "@fixers/nuxt-mail",
  "type": "module",
  "version": "1.0.1",
  "description": "A Nuxt module that makes mail easy",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/nuxt-mail.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@fixers/mail": "^1.0.0",
    "@nuxt/kit": "^3.5.2",
    "defu": "^6.1.2",
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@aws-sdk/client-ses": "^3.341.0",
    "@aws-sdk/credential-providers": "^3.341.0",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.5.1",
    "@types/node": "^18",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "changelogen": "^0.5.3",
    "eslint": "^8.41.0",
    "nodemailer": "^6.9.3",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nuxt": "^3.5.1",
    "vitest": "^0.31.1"
  }
}